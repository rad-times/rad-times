{
  "expo": {
    "name": "rad-times",
    "slug": "rad-times",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./assets/images/icon.png",
    "scheme": "radtimes",
    "userInterfaceStyle": "automatic",
    "backgroundColor": "#410707",
    "ios": {
      "supportsTablet": true,
      "config": {
        "googleMapsApiKey": ""
      },
      "bundleIdentifier": "com.davidryanhall.radtimes"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./assets/images/adaptive-icon.png",
        "backgroundColor": "#ffffff"
      },
      "config": {
        "googleMaps": {
          "apiKey": ""
        }
      },
      "permissions": [
        "android.permission.ACCESS_COARSE_LOCATION",
        "android.permission.ACCESS_FINE_LOCATION",
        "android.permission.CAMERA"
      ],
      "package": "com.davidryanhall.radtimes"
    },
    "web": {
      "bundler": "metro",
      "output": "static",
      "favicon": "./assets/images/favicon.png"
    },
    "plugins": [
      "expo-router",
      [
        "expo-splash-screen",
        {
          "backgroundColor": "#000000",
          "image": "./assets/images/splash-icon.png",
          "imageWidth": 300
        }
      ],
      [
        "expo-location",
        {
          "locationAlwaysAndWhenInUsePermission": "Allow $(PRODUCT_NAME) to use your location?"
        }
      ],
      [
        "expo-camera",
        {
          "cameraPermissionText": "Allow $(PRODUCT_NAME) access to your camera?"
        }
      ],
      [
        "expo-image-picker",
        {
          "photosPermission": "Allow $(PRODUCT_NAME) access to your photo library?",
          "cameraPermission": "Allow $(PRODUCT_NAME) access to your camera?"
        }
      ],
      [
        "@react-native-google-signin/google-signin",
        {
          "iosUrlScheme": ""
        }
      ],
      [
        "react-native-fbsdk-next",
        {
          "displayName": "Rad Times"
        }
      ]
    ],
    "experiments": {
      "typedRoutes": true
    },
    "extra": {
      "eas": {
        "projectId": "69b482d8-389b-4ac2-94a0-8e0c5a6bd2e7"
      },
      "ENV": "",
      "API_URL_ROOT": "",
      "WS_ROOT": "",
      "OAUTH_KEYS": {}
    }
  }
}
